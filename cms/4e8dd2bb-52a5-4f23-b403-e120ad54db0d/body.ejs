<script>
    let step=1;

    function onNextButtonClick() {
        console.log(arguments);
        if(step===6){
            return;
        }

        let currentURL, nextURL;

        currentURL="http://content.wxr.webizing.org/space/step0"+step;
        WXREvent.dispatchEvent({type: WXREvent.BEACON_MISSED, beacon: currentURL});

        step+=1;

        nextURL="http://content.wxr.webizing.org/space/step0" + step;
        WXREvent.dispatchEvent({type: WXREvent.BEACON_DETECTED, beacon: nextURL});
    }


    function onPreviousButtonClick() {
        if(step===0){
            return;
        }

        let currentURL, nextURL;

        currentURL="http://content.wxr.webizing.org/space/step0"+step;
        WXREvent.dispatchEvent({type: WXREvent.BEACON_MISSED, beacon: currentURL});

        step-=1;

        nextURL="http://content.wxr.webizing.org/space/step0" + step;
        WXREvent.dispatchEvent({type: WXREvent.BEACON_DETECTED, beacon: nextURL});
    }
</script>

