                  $('#anno04')[0].changedDomStyleDisplay('none');
                  setTimeout(function(){
                    $('#anno02')[0].changedDomStyleDisplay('block');
                  },500);
              }, 1000);
            }, 15000); 
            //Wait for pencil animation and pause for 1 second
          //After that, hide the pencil and show hex wrench
           



            trigger.ontrigger="";
       }

