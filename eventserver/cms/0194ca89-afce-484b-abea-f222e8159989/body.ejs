<script src="https://cdn.rawgit.com/webcomponents/webcomponentsjs/v1.1.0/webcomponents-lite.js"></script>
<link href="https://libs.webizing.org/wxr-0.5.html" rel="import">
<style>
	wxr-camera {
		--wxr-transform: translate3d(0, 0.5, 1) rotate3d(0, 0, 0);
	}

	wxr-light-ambient {
		--wxr-transform: translate3d(0, 0.5, 0);
	}

	wxr-light-directional {
		--wxr-transform: translate3d(0, 100, 10);
	}

	#box0 {

		--wxr-transform: translate3d(0, 0, 0) rotate3d(0, 0, 0);
	}

	#box1 {

		--wxr-transform: translate3d(0.5, 0.1, 0) rotate3d(0, 0, 0);
	}

	#box2 {

		--wxr-transform: translate3d(0, 0.1, 0) rotate3d(0, 0, 0);
	}

	#box3 {

		--wxr-transform: translate3d(-0.5, 0.1, 0) rotate3d(0, 0, 0);
	}


</style>

