<script src="https://cdn.rawgit.com/webcomponents/webcomponentsjs/v1.1.0/webcomponents-lite.js"></script>
<link href="https://libs.webizing.org/wxr-0.5.html" rel="import">
<style>
	wxr-camera {
		--wxr-transform: translate3d(0, 0.1, 0.6) rotate3d(0, 0, 0);
	}

	wxr-light-ambient {
		--wxr-transform: translate3d(0, 0.5, 0);
	}


	#prevBtn {
		width: 80px;
		height: 50px;
		border: 3px solid black;
	}

	#nexBtn {
		width: 80px;
		height: 50px;
		border: 3px solid black;

	}


	#annotation01 {
		--wxr-transform: translate3d(13, -3, 9) rotate3d(0, -0.9, 0) scale3d(15, 15, 15);
	}

	#annotation02 {
		--wxr-transform: translate3d(150, -3, 9) rotate3d(0.7, 0, 0) scale3d(150, 150, 150);
	}

	#annotation03 {
		--wxr-transform: translate3d(120, 80, -0.1) rotate3d(1, 0, 0) scale3d(150, 150, 150);
	}

	#annotation04 {
		--wxr-transform: translate3d(0.2, 0, -0.1) rotate3d(0, 0, 0) scale3d(0.3, 0.3, 0.3);
	}

	#annotation05 {
		--wxr-transform: translate3d(0.2, 0, -0.1) rotate3d(0, 0, 0) scale3d(10, 10, 10);
	}




	#model01_powder {
		--wxr-transform: translate3d(0, -3, 0) rotate3d(0, -0.7, 0) scale3d(100, 100, 100);
	}

	#model01_rightclip {
		--wxr-transform: translate3d(4, -13.5, 3.5) rotate3d(0, -0.7, 0) scale3d(100, 100, 100);
	}

	#model01_leftclip {
		--wxr-transform: translate3d(-4.5, -13.5, -3.5) rotate3d(0, -0.7, 0) scale3d(100, 100, 100);
	}

	#model01_rightclip_bottom {
		--wxr-transform: translate3d(4, -13.5, 3.5) rotate3d(0, -0.7, 0) scale3d(100, 100, 100);
	}

	#model01_leftclip_bottom {
		--wxr-transform: translate3d(-4.5, -13.5, -3.5) rotate3d(0, -0.7, 0) scale3d(100, 100, 100);
	}

	#model01_rightarrow {
		--wxr-transform: translate3d(6, -16, 3.5) rotate3d(0, 2.2, 2) scale3d(50, 50, 50);
	}

	#model01_leftarrow {
		--wxr-transform: translate3d(-7, -16, -3.5) rotate3d(0, -0.7, 2) scale3d(50, 50, 50);
	}



	#model02_plateassy{
		--wxr-transform: translate3d(-0.2, -0.2, -0.1) rotate3d(2.2, -0.6, 0) scale3d(1000, 1000, 1000);
	}

	#model02_screw{
		--wxr-transform: translate3d(-0.2, -0.2, -0.1) rotate3d(2.2, -0.6, 0) scale3d(1000, 1000, 1000);
	}



	#model03_1_screw{
		--wxr-transform: translate3d(-20, 500, -280) rotate3d(1.5078, -1.5078, 0) scale3d(3000, 3000, 3000);
	}

	#model03_1_arrow{
		--wxr-transform: translate3d(50, 400, -70) rotate3d(1.5078, 0, 0) scale3d(1500, 1500, 1500);
	}

	#model03_1_coupling{
		--wxr-transform: translate3d(-20, 500, -280) rotate3d(1.5078, -1.5078, 0) scale3d(3000, 3000, 3000);
	}

	#model03_couplingarrow{
		--wxr-transform: translate3d(-20, 500, -260) rotate3d(0, -1.5078, 0) scale3d(3000, 3000, 3000);
	}

	#model03_2_screw{
		--wxr-transform: translate3d(-20, 140, -260)  rotate3d(1.5078, 0, 0) scale3d(3000, 3000, 3000);
	}

	#model03_2_arrow{
		--wxr-transform: translate3d(-20, 30, -260)  rotate3d(1.5078, 0, 0) scale3d(2000, 2000, 2000);
	}




	#model04_bush{
		--wxr-transform: translate3d(-20, 140, -260) rotate3d(1.5078, 0, 0) scale3d(3000, 3000, 3000);
	}

	#model04_shaft{
		--wxr-transform: translate3d(-20, 140, -260) rotate3d(1.5078, 0, 0) scale3d(3000, 3000, 3000);
	}




	#model05_bush{
		--wxr-transform: translate3d(0, 0, 0) rotate3d(0, 1.5078, 0) scale3d(1, 1, 1);
	}

	#model05_shaft{
		--wxr-transform: translate3d(-0.3, 0, 0) rotate3d(0, 1.5078, 0) scale3d(1, 1, 1);
	}

	#model05_busharrow{
		--wxr-transform: translate3d(0.3, 0, 0) rotate3d(0, 1.5078, 0) scale3d(1, 1, 1);
	}

	#model05_shaftarrow{
		--wxr-transform: translate3d(-0.6, 0, 0) rotate3d(0, 1.5078, 0) scale3d(1, 1, 1);
	}
</style>

<wxr-world>
	<wxr-description
			title="insstek AR" img="https://wxr.nyc3.digitaloceanspaces.com/ar3dp/thumb/ar-thumb.png"
			desc="insstek 3D printer maintenance using WXR AR browser"></wxr-description>
	<wxr-mode default="3D" current="3D" modes='["3D", "AR"]'></wxr-mode>
	<wxr-ar engine="vuforia"></wxr-ar>

	<wxr-camera fovy=61 near=0.1 far=1000></wxr-camera>
	<wxr-space src="http://content.wxr.webizing.org/space/1" enable>

		<wxr-target id="STEP01" src="https://wxr.nyc3.digitaloceanspaces.com/ar3dp/target/insstek/01">
			<wxr-light-ambient color="0xf0f0f0" intensity="0.3"></wxr-light-ambient>
			<wxr-obj id="model01_powder" mtl="model01_powder.mtl" obj="model01_powder.obj" base-url="https://wxr.nyc3.digitaloceanspaces.com/ar3dp/resources/models/powder_ar">
				<wxr-animation type="movement" to="-0.2 0.1 -0.1" duration="2000" repeat=Infinity easing="linear"></wxr-animation>
			</wxr-obj>
			<wxr-obj id="model01_rightclip" mtl="model01_rightclip.mtl" obj="model01_rightclip.obj" base-url="https://wxr.nyc3.digitaloceanspaces.com/ar3dp/resources/models/powder_ar">
			</wxr-obj>
			<wxr-obj id="model01_leftclip" mtl="model01_leftclip.mtl" obj="model01_leftclip.obj" base-url="https://wxr.nyc3.digitaloceanspaces.com/ar3dp/resources/models/powder_ar">
			</wxr-obj>
			<wxr-obj id="model01_rightarrow" mtl="model01_rightarrow.mtl" obj="model01_rightarrow.obj" base-url="https://wxr.nyc3.digitaloceanspaces.com/ar3dp/resources/models/powder_ar"></wxr-obj>
			<wxr-obj id="model01_leftarrow" mtl="model01_leftarrow.mtl" obj="model01_leftarrow.obj" base-url="https://wxr.nyc3.digitaloceanspaces.com/ar3dp/resources/models/powder_ar"></wxr-obj>
			<wxr-plane id="annotation01" texture="https://wxr.nyc3.digitaloceanspaces.com/ar3dp/resources/dialog/powder_ar/step01.jpg"></wxr-plane>
		</wxr-target>


		<wxr-target id="STEP02" src="https://wxr.nyc3.digitaloceanspaces.com/ar3dp/target/insstek/02">
			<wxr-light-ambient color="0xf0f0f0" intensity="0.3"></wxr-light-ambient>
			<wxr-obj id="model02_plateassy" mtl="model02_plateassy.mtl" obj="model02_plateassy.obj" base-url="https://wxr.nyc3.digitaloceanspaces.com/ar3dp/resources/models/powder_ar">
				<wxr-animation type="movement" to="0 0 200" duration="4000" repeat=Infinity easing="linear"></wxr-animation>
			</wxr-obj>
			<wxr-obj id="model02_screw" mtl="model02_screw.mtl" obj="model02_screw.obj" base-url="https://wxr.nyc3.digitaloceanspaces.com/ar3dp/resources/models/powder_ar">
				<wxr-animation type="movement" to="0 -50 250" duration="4000" repeat=Infinity easing="linear"></wxr-animation>
			</wxr-obj>
			<wxr-plane id="annotation02" texture="https://wxr.nyc3.digitaloceanspaces.com/ar3dp/resources/dialog/powder_ar/step02.jpg"></wxr-plane>
		</wxr-target>


		<wxr-target id="STEP03" src="https://wxr.nyc3.digitaloceanspaces.com/ar3dp/target/insstek/03">
			<wxr-light-ambient color="0xf0f0f0" intensity="0.3"></wxr-light-ambient>
			<wxr-obj id="model03_1_coupling" mtl="model03_1_coupling.mtl" obj="model03_1_coupling.obj" base-url="https://wxr.nyc3.digitaloceanspaces.com/ar3dp/resources/models/powder_ar"></wxr-obj>
			<wxr-obj id="model03_couplingarrow" mtl="model03_couplingarrow.mtl" obj="model03_couplingarrow.obj" base-url="https://wxr.nyc3.digitaloceanspaces.com/ar3dp/resources/models/powder_ar"></wxr-obj>
			<wxr-obj id="model03_1_screw" mtl="model03_1_screw.mtl" obj="model03_1_screw.obj" base-url="https://wxr.nyc3.digitaloceanspaces.com/ar3dp/resources/models/powder_ar">
				<wxr-animation type="movement" to="30 500 -280" duration="2000" repeat=Infinity easing="linear"></wxr-animation>
			</wxr-obj>
			<wxr-obj id="model03_1_arrow" mtl="model03_1_arrow.mtl" obj="model03_1_arrow.obj" base-url="https://wxr.nyc3.digitaloceanspaces.com/ar3dp/resources/models/powder_ar"></wxr-obj>
			<wxr-obj id="model03_2_screw" mtl="model03_2_screw.mtl" obj="model03_2_screw.obj" base-url="https://wxr.nyc3.digitaloceanspaces.com/ar3dp/resources/models/powder_ar">
				<wxr-animation type="movement" to="-20 60 -260" duration="2000" repeat=Infinity easing="linear"></wxr-animation>
			</wxr-obj>
			<wxr-obj id="model04_bush" mtl="model04_bush.mtl" obj="model04_bush.obj" base-url="https://wxr.nyc3.digitaloceanspaces.com/ar3dp/resources/models/powder_ar">
				<wxr-animation type="movement" to="-20 100 -260" duration="2000" repeat=Infinity easing="linear"></wxr-animation>
			</wxr-obj>
			<wxr-obj id="model04_shaft" mtl="model04_shaft.mtl" obj="model04_shaft.obj" base-url="https://wxr.nyc3.digitaloceanspaces.com/ar3dp/resources/models/powder_ar">
				<wxr-animation type="movement" to="-20 100 -260" duration="2000" repeat=Infinity easing="linear"></wxr-animation>
			</wxr-obj>
			<wxr-obj id="model03_2_arrow" mtl="model03_2_arrow.mtl" obj="model03_2_arrow.obj" base-url="https://wxr.nyc3.digitaloceanspaces.com/ar3dp/resources/models/powder_ar">
				<wxr-animation type="movement" to="-20 -10 -260" duration="2000" repeat=Infinity easing="linear"></wxr-animation>
			</wxr-obj>
			<wxr-plane id="annotation03" texture="https://wxr.nyc3.digitaloceanspaces.com/ar3dp/resources/dialog/powder_ar/step03.jpg"></wxr-plane>
		</wxr-target>



		<wxr-target id="STEP04" src="https://wxr.nyc3.digitaloceanspaces.com/ar3dp/target/insstek/04">
			<wxr-light-ambient color="0xf0f0f0" intensity="0.5"></wxr-light-ambient>
			<wxr-obj id="model05_bush" mtl="model05_bush.mtl" obj="model05_bush.obj" base-url="https://wxr.nyc3.digitaloceanspaces.com/ar3dp/resources/models/powder_ar">
				<wxr-animation type="movement" to="45 0 0" duration="2000" repeat=Infinity easing="linear"></wxr-animation>
			</wxr-obj>
			<wxr-obj id="model05_shaft" mtl="model05_shaft.mtl" obj="model05_shaft.obj" base-url="https://wxr.nyc3.digitaloceanspaces.com/ar3dp/resources/models/powder_ar">
				<wxr-animation type="movement" to="10 0 0" duration="2000" repeat=Infinity easing="linear"></wxr-animation>
			</wxr-obj>
			<wxr-obj id="model05_busharrow" mtl="model05_busharrow.mtl" obj="model05_busharrow.obj" base-url="https://wxr.nyc3.digitaloceanspaces.com/ar3dp/resources/models/powder_ar">
				<wxr-animation type="movement" to="70 0 0" duration="2000" repeat=Infinity easing="linear"></wxr-animation>
			</wxr-obj>
			<wxr-obj id="model05_shaftarrow" mtl="model05_shaftarrow.mtl" obj="model05_shaftarrow.obj" base-url="https://wxr.nyc3.digitaloceanspaces.com/ar3dp/resources/models/powder_ar">
				<wxr-animation type="movement" to="-80 0 0" duration="2000" repeat=Infinity easing="linear"></wxr-animation>
			</wxr-obj>
			<wxr-plane id="annotation05" texture="https://wxr.nyc3.digitaloceanspaces.com/ar3dp/resources/dialog/powder_ar/step04.jpg"></wxr-plane>
		</wxr-target>


	</wxr-space>
</wxr-world>